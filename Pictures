		        <script type="text/javascript">

var ngApp = angular.module('ngINDEXApp', [
    'ngRoute',
    'ngResource',
    'ngTouch', 
    'ngAnimate',
    'ngSanitize',
    'ngCookies',
    'ui.bootstrap',
    'btLoginModule',
    'btMainModule',
    'btTemplates',
    'checklist-model',
    'btTemplatesPcorefront',
    'as.sortable',
	'svgMapModule',
    'jlareau.bowser',
    '720kb.socialshare'
])
.filter('trustAsHtml', function($sce) { return $sce.trustAsHtml; });

if (typeof _btFetchData != 'function') {

    ngApp.controller('ngFRONTNEWSController',[
    '$scope','$rootScope','btLoginService','$location','btMainService',
    function($scope,$rootScope,btLoginService,$location,btMainService) {
        
        $rootScope.init();
        
    }]);

} else {
	
    _btFetchData().then(function(response) {
    	
    	angular.module('btGetDataModule', [])
        .factory("btData", 
        function($http,$location){
            return response;
        })
        ;
    	
        ngApp.controller('ngFRONTNEWSController',[
        '$scope','$rootScope','btLoginService','$location','btMainService',
        function($scope,$rootScope,btLoginService,$location,btMainService) {
            
            $rootScope.init();
            
        }]);
        
        _btBootstrapApplication();
    
    });
    
}

</script><script type="text/javascript">

ngApp.controller('ngFRONTNEWSListController',[
'$scope','$anchorScroll','$location','$resource','$rootScope','$routeParams','$http','$timeout','btLoginService','$filter','btMainService',
function($scope,$anchorScroll,$location,$resource,$rootScope,$routeParams,$http,$timeout,btLoginService,$filter,btMainService) {
    
    angular.extend($scope, {
	    
        item: '',
        subitem: $routeParams.subitem
        
    });
    
    // Il possibile 'next page' non tiene tra i parametri l'eventuale filtro (tipo_notizia) delle news da mostrare
    // quindi tengo in memoria il parametro 'item' in $rootScope.news.section.
    // In getNews() assegno il suo valore alla variabile 'section' se quest'ultima si trova nulla (non definita o diversa da '').
    
    $rootScope.news.section = $scope.item;
    
    if($rootScope.news.section) {
    	console.log("--- news filtered by section:", $rootScope.news.section);
	}
	
    // ACTIONS
    ////////////////////////////////////////////////////////////////
    
    // reset window scroll (scrollTop)
    $rootScope.scrollTopWindow('page-content');
    
    $scope.fdata = '';
    
    //console.log("fdata: ");
	
	switch($scope.fdata) {
	
		case 'newslist':
	
			// reset

			// news.item
			$rootScope.arrayList.news.list = [];
		    
		    // pagination
		    $rootScope.currentPage.news.list = 0;
		    $rootScope.totalItems.news.list = 0;
		    
		    if($rootScope.news.append) {
			    $rootScope.pagedItems['news']['list'][0] = [];
		    }
		    
		    // ---
			
			// show news item (section)
			//console.log("CASE news list item:", $scope.item);
			$rootScope.getNews(null, $scope.item, null, null, true);
			break;
		
		case 'gallerylist':
			
			// show gallery list
			//console.log("CASE gallery list");
			$rootScope.getGallery(null, null, null, 'G');
			
			break;
		
		case 'calendarslist':
			
			// reset
			$rootScope.calendars.active = null;
			
			// ---
		
			// show calendars list
			//console.log("CASE calendars list");
			$rootScope.getGallery(null, null, null, 'X');
			break;
			
		default:

			break;
	
	}
}])
;

</script><div class="ng-view-container">

